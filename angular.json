{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Ekagra": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/ekagra",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules"
              ]
            },
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/theme.scss",
              "src/custom-theme.scss",
              "node_modules/slick-carousel/slick/slick.css",
              "node_modules/slick-carousel/slick/slick-theme.css",
              "node_modules/ol/ol.css",
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/select2/dist/css/select2.min.css",
              "node_modules/material-icons/iconfont/material-icons.css",
              "node_modules/material-icons/iconfont/outlined.css"
            ],
       "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/slick-carousel/slick/slick.min.js",
              "node_modules/signalr/jquery.signalR.min.js",
              "node_modules/select2/dist/js/select2.min.js"
            ],
            "allowedCommonJsDependencies": [
              "leaflet",
              "leaflet-draw",
              "moment",
              "highcharts",
              "highcharts/highcharts-more",
              "highcharts/modules/solid-gauge",
              "lodash.throttle",
              "lerc",
              "xml-utils/get-attribute.js",
              "xml-utils/find-tags-by-name.js"
            ],
            "server": "src/main.server.ts",
            "prerender": false,
            "ssr": false
          },
          "configurations": {
  "production": {
    "optimization": {
      "scripts": true,
      "styles": true,
      "fonts": false
    },
    "budgets": [
      {
        "type": "initial",
        "maximumWarning": "3mb",
        "maximumError": "5mb"
      },
      {
        "type": "anyComponentStyle",
        "maximumWarning": "3mb",
        "maximumError": "5mb"
      }
    ],
    "outputHashing": "all"
  },
  "development": {
    "optimization": false,
    "extractLicenses": false,
    "sourceMap": true
  }
},
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "Ekagra:build:production"
            },
            "development": {
              "buildTarget": "Ekagra:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": { "proxyConfig": "src/proxy.conf.json"}
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "Ekagra:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/leaflet.markercluster/dist/MarkerCluster.css",
              "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
              "node_modules/select2/dist/css/select2.min.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/select2/dist/js/select2.min.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}